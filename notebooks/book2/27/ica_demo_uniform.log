def plot_samples(S, title, file_name):
    min_x, max_x = -4, 4
    min_y, max_y = -3, 3
    plt.scatter(S[:, 0], S[:, 1], marker="o", s=16)
    plt.hlines(0, min_x, max_x, linewidth=2)
    plt.vlines(0, min_y, max_y, linewidth=2)
    plt.xlim(min_x, max_x)
    plt.ylim(min_y, max_y)
    plt.title(title)
