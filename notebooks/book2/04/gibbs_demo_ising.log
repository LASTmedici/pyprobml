pixelX = 100
pixelY = 100


def sigmoid(x):
    return 1 / (1 + np.exp(-x))


def energy(ix, iy, X, J):
    wi = 0
    if iy > 0:
        wi += X[iy - 1, ix]
    if iy < pixelY - 1:
        wi += X[iy + 1, ix]
    if ix > 0:
        wi += X[iy, ix - 1]
    if ix < pixelX - 1:
        wi += X[iy, ix + 1]
    return 2 * J * wi


def gibbs(rng, pixelX, pixelY, J, niter=50000):
    X = 2 * (rng.random((pixelX, pixelY)) > 0.5) - 1
    for iter in tqdm(range(niter)):

        ix = np.ceil((pixelX - 1) * rng.random(1)).astype(int)
        iy = np.ceil((pixelY - 1) * rng.random(1)).astype(int)

        e = energy(ix, iy, X, J)

        if rng.random(1) < sigmoid(e):
            X[iy, ix] = 1
        else:
            X[iy, ix] = -1
    return X


